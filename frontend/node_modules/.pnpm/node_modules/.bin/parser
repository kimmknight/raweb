#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules/@babel/parser/bin/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules/@babel/parser/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules/@babel/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules/@babel/parser/bin/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules/@babel/parser/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules/@babel/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/@babel+parser@7.27.2/node_modules:/home/runner/work/raweb/raweb/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
